echo 'm4_define([CASPAR_VERSION], ['`date +%Y%m%d`'])' > VERSION.m4
# compatibility with other mdcc.cx/ad1810 packages
date +%Y%m%d > VERSION

# zoem stuff
date +%e > stamp.day
date +%b > stamp.month
date +%Y > stamp.year

